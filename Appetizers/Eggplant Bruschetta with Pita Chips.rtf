\pard\tx220\tx720\pardeftab720\li720\fi-720\sl400\ql\qnatural
\ls2\ilvl0
\f1\b0\fs42 \cf0 {\listtext	\'95	}1 medium {\field{\*\fldinst{HYPERLINK "http://www.foodterms.com/encyclopedia/eggplant/index.html"}}{\fldrslt \cf2 eggplant}}, peeled, cut lengthwise into slices\
{\listtext	\'95	}4 tablespoons extra-virgin olive oil, for eggplant, plus 4 tablespoons, for {\field{\*\fldinst{HYPERLINK "http://www.foodterms.com/encyclopedia/bruschetta/index.html"}}{\fldrslt \cf2 bruschetta}} mix\
