\ls1\ilvl0
\b0\fs48 \cf0 {\listtext	\'95	}1 gallon carbonated water\
{\listtext	\'95	}2 1/2 cups sugar\
{\listtext	\'95	}2 teaspoons root beer concentrate\
\pard\tx220\tx720\pardeftab720\li720\fi-720\sl260\sa280\ql\qnatural
\ls2\ilvl0\cf0 {\listtext	\'95	}1 pound broken dry ice\
{\listtext	\'95	}Vanilla ice cream, for serving\
