\f2 \cf0 Ingredients\
\pard\tx940\tx1440\pardeftab720\li1440\fi-1440\sl320\sa120\ql\qnatural
\ls1\ilvl1
\f1\fs42 \cf0 		8 bone-in, skin-on chicken thighs \
		1/2 cup olive oil\
		1 lemon, juiced\
		Coarse salt and ground pepper\
		1 tablespoon grainy mustard\
		1 to 2 tablespoons honey\
\pard\tx220\tx720\pardeftab720\li720\fi-720\sl320\sa120\ql\qnatural
